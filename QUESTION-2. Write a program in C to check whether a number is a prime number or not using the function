#include <stdio.h>

    int primeNumber(int n){
    for (int i=2; i<=n/2;i++){
        if (n%i !=0)
        
        continue;
                 else
        return 1; 
        }
        
        return 0;
        }
    

      int main ()  {
          int n;
          printf("Enter n:");
          scanf("%d",&n);
         int ans = primeNumber(n);
         if(ans==0)
          printf("%d is prime number.",n);
          else
           printf("%d is not prime number.",n);
      }


